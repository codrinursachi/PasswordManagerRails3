<div id="<%= dom_id login %>">
  <p>
    <strong>Name:</strong>
    <%= login.name %>
  </p>

  <p>
    <strong>Login name:</strong>
    <%= login.login_name %>
  </p>

  <p>
    <strong>Login password:</strong>
    <%= login.login_password %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= login.notes %>
  </p>

  <p>
    <strong>Is favorite:</strong>
    <%= login.is_favorite %>
  </p>

  <p>
    <strong>Folder:</strong>
    <%= login.folder_id %>
  </p>

</div>
