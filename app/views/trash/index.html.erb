<%= link_to "Show all logins", logins_path %>
<% @logins.each do |login| %>
    <%= render login %>
    <p>
        <%= button_to "Permanently delete login", 'trash/' + login.id.to_s, method: :delete  %>
    </p>
    <p>
        <%= link_to "Restore login", 'trash/restore/' + login.id.to_s%>
    </p>
<% end %>